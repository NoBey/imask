!!! 5
html(lang='en')  
  head
    title= title
    script(type="text/javascript", src="https://ajax.googleapis.com/ajax/libs/jquery/1.6.3/jquery.min.js")
    -if (typeof(includes) != 'undefined')
      -includes.forEach(function (include) {
        -if (/\.js$/.test(include))
          script(src=include, type='text/javascript')
        -else if (/\.css$/.test(include))
          link(rel='stylesheet', type='text/css', href=include)
      -})
  body
    div(id="main")!= body